S' -> S

# 函数定义
S -> func funcs
funcs -> func funcs
funcs -> $
func -> type ID ( args ) func_body

# 返回类型
type -> int

# 参数类型
args -> type ID arg
args -> $
arg -> , type ID arg
arg -> $

# 函数体
func_body -> ;
func_body -> block
block -> { define_stmts }

# 定义语句
define_stmts -> define_stmt define_stmts
define_stmts -> $
define_stmt -> type ID init vars ;
