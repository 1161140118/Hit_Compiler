# 格式： A -> B C D
# P 过程
# D 声明
# S 可执行语句

####################

# 初始

P' -> P
P -> D S
D -> D D'
D' -> D
S -> S S

#####################

# 声明语句

D -> Dv
Dv -> T ID ;
T -> Type C

Type -> int
Type -> char
Type -> float
Type -> void

## 数组下标
C -> [ INT ] C
C -> $

#####################

# 赋值语句

S -> ID = E ;
S -> L = E ;

E -> E + T
E -> T
T -> T * F
T -> F
F -> ( E )
E -> ID
E -> const
E -> L

L -> ID [ E ]
L -> L [ E ]

const -> INT
const -> FLOAT
const -> BOOL
const -> STR

#####################

# 控制语句
# B表示布尔表达式
