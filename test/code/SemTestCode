int a;
int b;

func f1(){
    c= 1;
    a = b + c;
}

func f2(){
    int d;
    d = 1;
}
    
func main(){
    int c;
    int d;
    int y;
    float z;
    a = 1;
    b = 2;
   
    while (a<b) do {
        if (c<d) then {
            x = y + z;
